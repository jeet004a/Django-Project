{
    "builds": [
      {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src":"build_files.sh",
        "use": "@vercel/static",
        "config": {
            "distDir": "static"
        }
      }
    ],
    "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
    {
        "src": "/(.*)",
        "dest": "mysite/wsgi.py"
    }
    ]
}